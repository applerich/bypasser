{
  "manifest_version": 2,

  "name": "Challenge Bypasser",
  "description": "Bypass cloudflare's captchas",
  "version": "1.0",

  "background": {
    "scripts": [
      "lib/bn.js",
      "src/blind.js",
      "src/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "skin/icon.png",
    "default_popup": "skin/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "webRequest",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
