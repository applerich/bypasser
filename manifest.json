{
  "manifest_version": 2,

  "name": "Challenge Bypasser",
  "description": "Bypass cloudflare's captchas",
  "version": "1.0",

  "background": {
    "scripts": [
      "lib/prng4.js",
      "lib/rng.js",
      "lib/jsbn.js",
      "lib/jsbn2.js",
      "lib/rsa.js",
      "lib/rsa2.js",
      "src/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "skin/icon.png",
    "default_popup": "skin/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
